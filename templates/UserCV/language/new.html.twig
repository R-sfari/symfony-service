<button type="button" class="uk-modal-close uk-close"></button>
<div class="uk-modal-header">
  <h3 class="uk-modal-title">{{ 'user_cv.language.new'|trans }}</h3>
</div>
<div class="uk-animation-fade">
  {{ form_start(form ,{ 'action': path('language_new') }) }}
  <div class="uk-form-row">
    <label class="uk-form-label">{{ form_label(form.language) }}</label>
    {{ form_widget(form.language)}}
    <div class="parsley-errors-list filled uk-text-danger">
      {{ form_errors(form.language) }}
    </div>
  </div>
  <div class="uk-form-row">
    <label class="uk-form-label">{{ form_label(form.proficiency) }}</label>
    {{ form_widget(form.proficiency)}}
    <div class="parsley-errors-list filled uk-text-danger">
      {{ form_errors(form.proficiency) }}
    </div>
  </div>
  <div class="uk-modal-footer uk-text-right">
    <button type="button" class="md-btn md-btn-flat uk-modal-close">Close</button>
    {{ form_widget(form.submit) }}
  </div>
  {{ form_widget(form) }}
  {{ form_end(form) }}
</div>
