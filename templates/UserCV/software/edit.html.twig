<button type="button" class="uk-modal-close uk-close"></button>
<div class="uk-modal-header">
  <h3 class="uk-modal-title">{{ 'user_cv.software.edit'|trans }}</h3>
</div>
<div class="uk-animation-fade">
  {{ form_start(form ,{ 'action': path('software_edit', {'id' : software.id }) }) }}
  <div class="uk-form-row">
    <label class="uk-form-label">{{ form_label(form.skill) }}</label>
    {{ form_widget(form.skill)}}
    <div class="parsley-errors-list filled uk-text-danger">
      {{ form_errors(form.skill) }}
    </div>
  </div>
  <div class="uk-form-row">
    <label class="uk-form-label">{{ form_label(form.progress) }}</label>
    {{ form_widget(form.progress)}}
    <div class="parsley-errors-list filled uk-text-danger">
      {{ form_errors(form.progress) }}
    </div>
  </div>
  <div class="uk-modal-footer uk-text-right">
    <button type="button" class="md-btn md-btn-flat uk-modal-close">Close</button>
    {{ form_widget(form.submit) }}
  </div>
  {{ form_widget(form) }}
  {{ form_end(form) }}
</div>
